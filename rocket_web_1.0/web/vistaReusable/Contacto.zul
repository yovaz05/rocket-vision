      </row> 
      <row visible="false">        
        <label value="Twitter" sclass="etiqueta_campo"/>
        <toolbarbutton  id="tbbTwitter" label=""/>
      </row> 
    </rows>
  </grid>
  
  <grid id="gridEdit" visible="false" style="border:0">
    <columns>
      <column width="80px"/>
      <column/>
    </columns>
    <rows>
      <row>        
        <label value="Celular" sclass="etiqueta_campo"/>
