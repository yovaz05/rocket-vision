      </row> 
      <row>        
        <label value="Email" sclass="etiqueta_campo"/>
        <textbox id="txtEmail" value="" cols="35"/>
      </row> 
      <row>        
        <label value="Facebook" sclass="etiqueta_campo"/>
        <textbox id="txtFacebook" value="" cols="35"/>
      </row> 
      <row>        
        <label value="Twitter" sclass="etiqueta_campo"/>
        <textbox id="txtTwitter" value="" cols="35"/>
      </row> 
    </rows>
  </grid>  
