<?xml version="1.0" encoding="UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zul">
  <grid style="border:0px">      
    <columns>
      <column id="col1" width="180px"/>
      <column id="colEdit" visible="false"/>
      <column id="colView"/>
    </columns>
    <rows>
      <row>
        <label value="Nuevos Invitados" sclass="etiqueta_campo" />
        <spinner id="spnInvitados" cols="2" width="50px" value="0" constraint="min 0 max 100" />
        <label id="etqInvitados" value="0"/>
      </row>        
      <row>
        <label value="Por Reconciliarse" sclass="etiqueta_campo"/>
        <spinner id="spnReconciliados" cols="2" width="50px" value="0" constraint="min 0 max 100"/>
        <label id="etqReconciliados" value="0"/>
      </row>
      <row>
        <label value="Visitas a Realizar" sclass="etiqueta_campo"/>
        <spinner id="spnVisitas" cols="2" width="50px" value="0" constraint="min 0 max 100"/>
        <label id="etqVisitas" value="0"/>
      </row>           
      <row>
        <label value="Personas en Planificación" sclass="etiqueta_campo" tooltiptext="Líderes + Estacas"/>
        <spinner id="spnPersonasEnPlanif" cols="2" width="50px" value="0" constraint="min 0 max 100"/>
        <label id="etqPersonasEnPlanif" value="0"/>
      </row>
    </rows>
  </grid>

</zk>
