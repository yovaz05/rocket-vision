<?xml version="1.0" encoding="UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
  <style src="css/estilos1.css"/>
  <div apply="cdo.sgd.controladores.CtrlLiderLanzadoListado" width="825px">
    <label value="Discípulos Lanzados" sclass="titulo_ventana"/>
    <toolbar width="100%" height="20px" sclass="barraBotones"  visible="false">
      <toolbarbutton id="btnNew" label="Nuevo" sclass="botonAccion"/>
    </toolbar>
    <div align="center">
      <grid id="grid" style="border:normal" mold="paging" pageSize="5">
        <columns sizable="true">
          <column label="#"         width="25px"/>
          <column label="Nombre"    width="150px"/>
          <column label="Dirección" width="200px"/>
          <column label="Teléfono"  width="100px"/>
          <column label="Email"     width="200px"/>
          <column label="Líderes"/>
        </columns>
      </grid>
    </div>
  </div>
</zk>
